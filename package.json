{
  "type": "module",
  "scripts": {
    "build:main": "rollup --config rollup.main.mjs --configPlugin typescript --bundleConfigAsCjs && npm run generate:main",
    "build:atoms": "rollup --config rollup.atom.mjs --configPlugin typescript --bundleConfigAsCjs && npm run generate:atoms",
    "build:atoms:pkg": "rollup --config rollup.atom-pkg.mjs --configPlugin typescript --bundleConfigAsCjs",
    "build:basic": "rollup --config rollup.basic.mjs --configPlugin typescript --bundleConfigAsCjs && npm run generate:basic",
    "build:basic:pkg": "rollup --config rollup.basic-pkg.mjs --configPlugin typescript --bundleConfigAsCjs",
    "lint:fix": "eslint . --fix",
    "format": "prettier -w .",
    "cli": "node cli/index.cjs",
    "generate:main": "node scripts/generateMainPackage.cjs",
    "generate:atoms": "node scripts/generateSubPackage.cjs atoms",
    "generate:basic": "node scripts/generateSubPackage.cjs basic"
  },
  "devDependencies": {
    "@babel/core": "7.26.10",
    "@babel/plugin-transform-runtime": "7.26.10",
    "@babel/preset-env": "7.26.9",
    "@rollup/plugin-alias": "5.1.1",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "28.0.3",
    "@rollup/plugin-data-uri": "1.0.3",
    "@rollup/plugin-dsv": "3.0.5",
    "@rollup/plugin-dynamic-import-vars": "2.1.5",
    "@rollup/plugin-image": "3.0.3",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-multi-entry": "6.0.1",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.2",
    "@types/node": "22.13.10",
    "@types/react": "18.2.0",
    "autoprefixer": "10.4.21",
    "commander": "13.1.0",
    "eslint": "9.22.0",
    "eslint-config-unjs": "0.4.2",
    "inquirer": "8.2.5",
    "jiti": "2.4.2",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "rollup": "4.35.0",
    "rollup-plugin-dts": "6.1.1",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-scss": "4.0.0",
    "sass": "1.85.1",
    "tslib": "2.8.1",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@jd/sgm-mp": "4.0.0",
    "@jdreact/jdreact-core-lib": "3.0.31",
    "@tarojs/components": "3.6.35",
    "@tarojs/plugin-platform-h5": "3.6.35",
    "@tarojs/taro": "3.6.35",
    "react": "18.2.0",
    "react-native": "0.72.3"
  }
}
